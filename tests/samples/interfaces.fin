// 1. Define an Interface
interface Printable {
    pub fun to_string() <string>;
}

// 2. Define a Struct that "should" implement it (we don't check impl yet, just syntax)
struct User {
    name <string>,
    age <int>
}

// 3. Function with Generic Constraint
// T must implement Printable (checked in Semantics)
fun print_any<T: Printable>(item: <T>) <noret> {
    let s <string> = item.to_string();
    printf("%s", s);
}

fun main() <noret> {
    let u <User> = User { name: "Fin", age: 1 };
    // This would technically fail in a real compiler if User doesn't impl Printable
    // But for now, we just want to see if Semantics accepts the syntax.
    print_any::<User>(u); 
}
